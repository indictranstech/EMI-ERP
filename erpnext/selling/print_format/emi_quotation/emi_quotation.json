 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n    {% if letter_head and not no_letterhead %}\n    <div class=\"letter-head\">{{ letter_head }}</div>\n    {% endif %}\n    {%- if doc.meta.is_submittable and doc.docstatus==0-%}\n    <div class=\"alert alert-info text-center\">\n        <h4 style=\"margin: 0px;\">{{ _(\"PROFORMA\") }}</h4></div>\n    {%- endif -%}\n    {%- if doc.meta.is_submittable and doc.docstatus==2-%}\n    <div class=\"alert alert-danger text-center\">\n        <h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4></div>\n    {%- endif -%}\n    {% if max_pages > 1 %}\n    <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n    {% endif %}\n{%- endmacro -%}\n\n<small>\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\n<big>\n<p class=\"text-center\"><b>{{ _(\"QUOTATION\") }}</b></p><br>\n</big>\n<div class=\"row\">        \n    \n    <div class=\"col-xs-6\">            \n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"><big><b>Customer Name</b></big></div>\n            <div class=\"col-xs-7 \"><big>{{ doc.customer }}</big> </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"><big><b>Address</b></big></div>\n            <div class=\"col-xs-7 \"><big>{{ doc.address_display or ''}}</big></div>\n        </div>\n        {%- if doc.po_no -%}\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\"><big><b>PO No</b></big></div>\n                <div class=\"col-xs-7 \"><big>{{ doc.po_no or ''}}</big> </div>\n            </div>\n        {%- endif -%}\n\n        {%- if doc.po_date -%}\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\"> <big><b>PO Date</b></big> </div>\n                <div class=\"col-xs-7 \">\n                <big>{{ doc.get_formatted(\"po_date\") or ''}}</big></div>\n            </div>  \n        {%- endif -%}\n\t\n   <div class=\"row\">\n                <div class=\"col-xs-5 text-right\"> <big><b>Lead Owner Name</b></big> </div>\n                <div class=\"col-xs-7 \">\n                <big>{{ doc.lead_owner_name}}</big></div>\n            </div>  \n  <div class=\"row\">\n                <div class=\"col-xs-5 text-right\"> <big><b>Mobile Number</b></big> </div>\n                <div class=\"col-xs-7 \">\n                <big>{{ doc.mobile_no}}</big></div>\n            </div>  \n\n\n    </div>\n\n    <div class=\"col-xs-6\">\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"><big><b>Order No</b></big></div>\n            <div class=\"col-xs-7 \"><big>{{ doc.name or ''}}</big> </div>\n        </div>\n        \n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"> <big><b>Date</b></big> </div>\n            <div class=\"col-xs-7 \">\n                <big>{{ doc.get_formatted(\"transaction_date\") or ''}}</big></div>\n\t    </div>    \n        \n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"><big><b>Enquiry Type</b></big></div>\n            <div class=\"col-xs-7 \"><big>{{ doc.enquiry_type }}</big> </div>\n        </div>\n\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"> <big><b>Enquiry No</b></big> </div>\n            <div class=\"col-xs-7 \">\n                <big>{{ doc.enquiry_no}}</big></div>    \n\t    </div>\n\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"> <big><b>Enquiry Date</b></big> </div>\n            <div class=\"col-xs-7 \">\n                <big>{{ doc.enquiry_date}}</big></div>\n\t    </div>\n    </div>       \n</div>\n<br>\n<div class=\"row\">\n    <table class=\"table table-condensed table-hover table-bordered\">\n        <tbody>\n            <tr>\n                <th>Sr</th>\n                <th>Description</th>\n                <th class=\"text-right\">Qty</th>\n                <th>Unit</th>  \n    \t        <th class=\"text-right\">Unit Price<br>({{(doc.currency)}})</th>\n                <th class=\"text-right\">Total Price<br>({{(doc.currency)}})</th>\n            </tr>\n            {%- for row in doc.items -%}\n                <tr>\n                    <td style=\"width: 3%;\">{{ row.idx }}</td>\n                    <td style=\"width: 57%;\"><b>{{ row.description or '' }} </b><br></td>\n                    <td style=\"width: 10%; text-align: right;\">{{ row.qty }}</td>\n                    <td style=\"width: 10%; text-align: right;\">{{ row.stock_uom }}</td>\n                    <td style=\"width: 10%; text-align: right;\">{{ row.rate }}</td>\n                    <td style=\"width: 10%; text-align: right;\">{{ row.amount }}</td>\n                </tr>\n            {%- endfor -%}\n     \t    <tr>\n    \t       <td colspan=4> Total </td> <td></td>\n    \t       <td style=\"width: 10%; text-align: right;\">{{ doc.total }}</td>\n    \t    </tr>\t   \n        </tbody>\n    </table>\n</div>\n<br> \n<div class=\"row\">\n{%- if doc.additional_discount_percentage -%}\n    <div class=\"row\"> \n\t <div class=\"col-xs-7 text-left\"><big><b></b></big></div>\n        <div class=\"col-xs-3 text-left\"><big><b>Discount Percentage</b></big></div>\n        <div class=\"col-xs-2 text-left\">{{ doc.additional_discount_percentage}}</div>\n    </div>\n{%- endif -%}\n{%- if doc.discount_amount -%}\n    <div class=\"row\">\n\t <div class=\"col-xs-7 text-left\"><big><b></b></big></div>\n        <div class=\"col-xs-3 text-left\"><big><b>Discount Amount</b></big></div>\n        <div class=\"col-xs-2 text-left\">{{ doc.discount_amount or ''}}</div>\n    </div>\n{%- endif -%}\n\t<div class=\"row\">\n\t\t <div class=\"col-xs-7 text-left\"><big><b></b></big></div>\n        \t<div class=\"col-xs-3 text-left\"><big><b>Net Total</b></big></div>\n        \t<div class=\"col-xs-2 text-left\">{{ doc.net_total}}</div>\n       </div>\n</div>\n<br>\n<div class=\"row\">\n{%- if doc.terms -%}\n    <div class=\"row\">\n        <div 