 "doctype": "Print Format", 
 "font": "Default", 
 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n    {% if letter_head and not no_letterhead %}\n    <div class=\"letter-head\">{{ letter_head }}</div>\n    {% endif %}\n    {%- if doc.meta.is_submittable and doc.docstatus==0-%}\n    <div class=\"alert alert-info text-center\">\n        <h4 style=\"margin: 0px;\">{{ _(\"PROFORMA\") }}</h4></div>\n    {%- endif -%}\n    {%- if doc.meta.is_submittable and doc.docstatus==2-%}\n    <div class=\"alert alert-danger text-center\">\n        <h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4></div>\n    {%- endif -%}\n    {% if max_pages > 1 %}\n    <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n    {% endif %}\n{%- endmacro -%}\n\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\n\n<div class =\"alert alert-danger text-center\">\n<div class=\"print-heading\">\n        <h2 class=\"text-center\">Delivery Note<br>\n            <small>{{ doc.name or ''}}</small>\n        </h2>\n</div>\n</div>\n<div class=\"alert\" style=\"border: solid 1px\">\n\n<div class=\"row\">        \n     <div class=\"col-xs-6\">            \n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"><label>Customer Name</label></div>\n            <div class=\"col-xs-7 \">{{ doc.customer }} </div>\n        </div>\n        {%- if doc.address_display -%}\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-right\"><label>Address</label></div>\n            <div class=\"col-xs-7 \">{{ doc.address_display or ''}}</div>\n        </div>         \n        {%- endif -%}\n    </div>\n\n   \n\n    <div class=\"col-xs-6\">\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-left\"> <label>Date</label> </div>\n            <div class=\"col-xs-7 \">\n                {{ doc.get_formatted(\"posting_date\") or ''}}</div>\n        </div>{%- if doc.po_no -%}\n\t<div class=\"row\">\n            <div class=\"col-xs-5 text-left\"> <label>L.P.O No</label> </div>\n            <div class=\"col-xs-7 \">{{ doc.po_no or ''}}</div>\n        </div>     {%- endif %}  {%- if doc.items -%} <div class=\"row\"><div class=\"col-xs-5 text-left\"><label>Sales Order No</label></div>\n            <div class=\"col-xs-7 \">{{ doc.items[0].against_sales_order}}</div>\n</div> {%- endif -%}   \n    </div>\n    </div>\n</div>         \n \n  \n<br>\n<table class=\"table table-condensed table-hover table-bordered\">\n        <tr>\n            <th>Sr</th>\n            <th>Item Code</th>\n            <th>Item Name</th>\n            <th>UNIT</th>\n            <th class=\"text-right\">Quantity </th>\n        </tr>\n        {%- for row in doc.items -%}\n        <tr>\n            <td style=\"width: 10%;\">{{ row.idx }}</td>\n            <td style=\"width: 40%;\">\n        {% if row.in_format_data(\"image\") and row.get(\"image\") -%}\n            <div class=\"pull-left\" style=\"max-width: 20%; margin-right: 10px;\">\n                <img src=\"{{ row.image }}\" style=\"max-width: 100%\">\n            </div>\n        {%- endif %}\n\n                <b>{{ row.item_code or '' }} </b><br> </td>\n               <td style=\"width: 20%; text-align: left;\">  {{ row.description or '' }}  </td><br></td>\n                <td> {{ row.stock_uom or '' }}  </td>\n            <td style=\"width: 20%; text-align: center;\">{{ row.qty }}</td>\n        </tr>\n        {%- endfor -%}\n    \n</table>\n\n<br>\n<div class=\"row\">\n{%- if doc.transporter_name -%}\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-left\"><label>Transporter Name</label></div>\n            <div class=\"col-xs-7 \">{{ doc.transporter_name}}</div>\n        </div>         \n {%- endif -%}\n\n{%- if doc.lr_no -%}\n        <div class=\"row\">\n            <div class=\"col-xs-5 text-left\"><label>Vehicle No</label></div>\n            <div class=\"col-xs-7 \">{{ doc.lr_no}}</div>\n        </div>         \n {%- endif -%}\n</div>\n{%- if doc.terms -%}\n<div class=\"row\">\n <br><b>Terms & Conditions:</b>\n <div class=\"col-xs-12 text-left\">{{ doc.terms or '' }}</div>\n</div> {%- endif -%}\n<br><br>\n<div class=\"row\">\n   <div class=\"col-xs-6\">\n      <div class=\"row\">\n         <div class=\"col-xs-7 text-right\"><big>---------------------------</big></div>\n         <div class=\"col-xs-5 \"> </div>\n      </div>\n   </div>\n   <div class=\"col-xs-6\">\n      <div class=\"row\">\n         <div class=\"col-xs-4 \"></div>\n         <div class=\"col-xs-2 text-right\"></div>\n         <div class=\"col-xs-6 \"><big>--------------------------</big></div>\n      </div>\n   </div>\n</div>\n\n<div class=\"row\">\n   <div class=\"col-xs-6\">\n      <div class=\"row\">\n         <div class=\"col-xs-7 text-right\"><big>Authorised Signatory</big></div>\n         <div class=\"col-xs-5 \"></div>\n      </div>\n   </div>\n   <div class=\"col-xs-6\">\n      <div class=\"row\">\n         <div class=\"col-xs-4 \"><big></big></div>\n         <div class=\"col-xs-2 text-right\"><big></big></div>\n         <div class=\"col-xs-6 \"><big>Receivers Signature</big></div>\n      </div>\n   </div>\n</div>\n\n", 
